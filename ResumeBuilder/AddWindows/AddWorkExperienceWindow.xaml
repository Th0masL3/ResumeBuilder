<Window x:Class="ResumeBuilder.AddWindows.AddWorkExperienceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResumeBuilder.AddWindows"
        mc:Ignorable="d"
        Title="AddWorkExperienceWindow" Height="450" Width="800">
    <StackPanel Margin="10">
        <TextBlock Text="Company Name" Margin="5"/>
        <TextBox x:Name="CompanyNameTextBox" Margin="5"/>

        <TextBlock Text="Role" Margin="5"/>
        <TextBox x:Name="RoleTextBox" Margin="5"/>

        <TextBlock Text="Years Worked" Margin="5"/>
        <TextBox x:Name="YearsWorkedTextBox" Margin="5"/>

        <Button Content="Submit" Margin="5" Click="SubmitButton_Click"/>
    </StackPanel>
</Window>
